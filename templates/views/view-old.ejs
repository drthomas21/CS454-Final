<div class="col-lg-12" ng-controller="WishListCtrl" ng-init="search()">
	<div class="row">
		<div class="col-md-12">
			<h2>{{Model.name}}<br /><small>As Of: {{Model.date | date : "medium"}}</small></h2>
		</div>
	</div>
	<div class="row">
		<div class="col-md-6">
			<label>Description:</label>
			<p ng-bind="Model.description"></p>
		</div>
		<div class="col-md-6">
			<label>Items:</label>
			<ul>
				<li ng-repeat="item in Model.items" ng-bind="item"></li>
			</ul>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<button class="btn btn-warning" ng-click="editModel()">Edit</button>
		</div>
	</div>
</div>